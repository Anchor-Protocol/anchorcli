    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "multiplerun": "^1.1.2",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "types": "./dist/index.d.ts"
