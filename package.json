    "clear": "^0.1.0",
    "commander": "^6.2.0",
    "dot-prop": "^6.0.1",
    "figlet": "^1.5.0",
    "jsome": "^2.5.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.20",
    "yaml": "^1.10.0",
    "yesno": "^0.3.1"
  },
  "devDependencies": {
    "@ssen/eslint-config": "^1.3.1",
    "@ssen/prettier-config": "^1.2.0",
